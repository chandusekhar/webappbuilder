                  },
                  "valueObj": {
                    "value": ""
                  },
                  "prompt": "Zoning Type is",
                  "textsearchhint": "Example: RESIDENTIAL",
                  "sqltext": "ZONING_TYPE = '[value]'",
                  "operation": "stringOperatorIs",
                  "uniquevalsfromfield": "ZONING_TYPE"
                },
                {
                  "fieldObj": {
                    "name": "ZONING_NAME",
                    "label": "ZONING_NAME",
